        <layer name="informix-datasource"/>
    </dependencies>
    
    <feature spec="subsystem.ee.service.default-bindings">
        <param name="datasource" value="java:jboss/datasources/${env.INFORMIX_DATASOURCE:INFORMIXDS}"/>
    </feature>
</layer-spec>